{
  "manifest_version": 3,
  "name": "Cumuli AWS Console Chat",
  "version": "0.1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/logo16.png",
    "48": "icons/logo48.png",
    "128": "icons/logo128.png"
  },
  "permissions": ["activeTab"],
  "host_permissions": [
    "*://console.aws.amazon.com/*",
    "*://*.console.aws.amazon.com/*"
  ]
}
