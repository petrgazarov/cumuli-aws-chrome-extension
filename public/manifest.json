{
  "manifest_version": 3,
  "name": "Cumuli - AWS Console Chat",
  "description": "Chat with GPT-4 turbo on any AWS page",
  "version": "0.1.7",
  "action": {
    "default_title": "Cumuli - AWS Console Chat"
  },
  "background": {
    "service_worker": "js/serviceWorker.js"
  },
  "icons": {
    "16": "icons/logo16.png",
    "48": "icons/logo48.png",
    "128": "icons/logo128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; style-src 'self' 'unsafe-inline'"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "tabs",
    "activeTab",
    "sidePanel"
  ],
  "commands": {
    "submit_with_screenshot": {
      "suggested_key": "Shift+Ctrl+I",
      "description": "Submit message with screenshot"
    },
    "_execute_action": {}
  }
}
