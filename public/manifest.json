{
  "manifest_version": 3,
  "name": "Cumuli AWS Console Chat",
  "version": "0.1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/Drawer.js"]
    }
  ],
  "icons": {
    "16": "icons/logo16.png",
    "48": "icons/logo48.png",
    "128": "icons/logo128.png"
  },
  "host_permissions": [
    "*://console.aws.amazon.com/*",
    "*://*.console.aws.amazon.com/*"
  ]
}
